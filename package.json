  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-local": "cross-env STAGE=dev ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:dev-server": "cross-env STAGE=dev ENV=server node dist/main",
    "start-linux:dev-server": "STAGE=dev ENV=server node dist/main.js",
    "start:preprod-server": "cross-env STAGE=preprod ENV=server node dist/main",
    "start-linux:preprod-server": "STAGE=preprod ENV=server node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "cross-env STAGE=dev ENV=local ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm -- migration:create src/database/migrations/%npm_config_name%",
    "migration:generate": "npm run build && npm run typeorm -- -d src/database/typeorm.datasource.ts migration:generate src/database/migrations/%npm_config_name%",
    "migration:run": "npm run build && npm run typeorm -- -d src/database/typeorm.datasource.ts migration:run",
    "migration:revert": "npm run typeorm -- -d src/database/typeorm.datasource.ts migration:revert"
  },
